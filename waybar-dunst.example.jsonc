// Example waybar configuration
{
    "custom/dunst": {
        "return-type": "json",
        "format": "{text}",
        // Change /usr/local to /usr when installed with a package manager
        "exec": "/usr/local/lib/waybar-dunst/waybar-dunst",
        "on-click": "dunstctl set-paused toggle"
    }
}
